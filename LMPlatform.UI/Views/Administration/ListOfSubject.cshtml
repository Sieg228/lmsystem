@model LMPlatform.UI.ViewModels.AdministrationViewModels.ListSubjectViewModel


<div id="adminModal">

    <table class="table table-bordered table-striped table-hover">
        <caption> @Model.Name</caption>
        <thead>
            <tr>
                <th>Название</th>
                <th>Преподаватель</th>
            </tr>
        </thead>
        <tbody>
        @foreach (var i in Model.Subjects)
            {
                <tr>

                    <td>@i.Name</td>
                    @if (i.SubjectLecturers != null)
                    {
                        <td>@foreach (var prof in i.SubjectLecturers)
                            {
                                @prof.Lecturer.FullName
								<br/>
                            }</td>
                    }
                </tr>
            }
        </tbody>
    </table>

</div>

<script type="text/javascript">
    $('#editStudent a').tooltip(
    {
        placement: "right",
    });
</script>